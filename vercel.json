{
  "framework": "vite",
  "buildCommand": "cd dashboard/frontend && npm install && npm run build",
  "outputDirectory": "dashboard/frontend/dist",
  "redirects": [
    { "source": "/", "destination": "/lite", "permanent": false }
  ],
  "rewrites": [
    { "source": "/lite", "destination": "/index.html" },
    { "source": "/lite/(.*)", "destination": "/index.html" }
  ]
}
